{
    "name": "admin/dashboard",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": " ^2.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.5",
        "spatie/laravel-activitylog": "^4.3",
        "tonysm/turbo-laravel": "^1.0"
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "^2.8",
        "brianium/paratest": "^6.3",
        "enlightn/security-checker": "^1.9",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.1",
        "jcergolj/laravel-form-request-assertions": "^1.0",
        "jcergolj/laravel-view-test-assertions": "^2.0",
        "johnkary/phpunit-speedtrap": "^4.0",
        "laravel/breeze": "^1.1",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^1.0",
        "nunomaduro/phpinsights": "^2.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": ["app/helpers.php", "tests/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "jsprettier": "npm run format",
        "phpstan": "./vendor/bin/phpstan analyse --memory-limit=2G",
        "insights": "@php artisan insights --no-interaction --min-quality=80 --min-complexity=80 --min-architecture=80 --min-style=80",
        "phpcs": "vendor/bin/php-cs-fixer fix",
        "analyse": [
            "@phpcs",
            "@jsprettier",
            "@insights",
            "@phpstan"
        ]
    }
}
